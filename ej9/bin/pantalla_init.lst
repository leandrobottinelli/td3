   453                                  
   454                                  ; Recorrer bus PCI. Bochs solo soporta bus cero ya que no tiene "PCI to PCI bridge".
   455                                  ; Cada bus tiene hasta 32 dispositivos.
   456                                  ; Si el dispositivo es compuesto puede tener hasta 8 funciones.
   457                                  ; Formato puerto 0x0CF8:
   458                                  ; Bit 31 = Siempre a "1"
   459                                  ; Bits 30-24 = Reservado (siempre a "0")
   460                                  ; Bits 23-16 = Número de bus (0 en nuestra aplicación)
   461                                  ; Bits 15-11 = Número de dispositivo (0-31)
   462                                  ; Bits 10-8 = Número de función (0-7)
   463                                  ; Bits 7-2 = Número de registro
   464                                  ; Bits 1-0: Siempre cero.
   465                                  
   466                                  recorrer_bus_pci:
   467 000010FF 66BD00400000                mov     ebp, 0x4000             ; Primera dirección de entrada/salida para PCI.
   468 00001105 66BF000000C0                mov     edi, 0xC0000000         ; Primera dirección a asignar a dispositivos PCI.
   469 0000110B 66BE00000080                mov     esi, 0x80000000         ; Bus 0, dispositivo 0, función 0.
   470                                  
   471                                  ciclo_recorrer_bus_pci:
   472 00001111 6681FE00000180              cmp     esi, 0x80010000         ; Verificar si se recorrió todo el bus 0.
   473 00001118 0F843001                    jz      fin_recorrido_pci
   474                                  
   475                                  ; Leer registro 0: Bits 31-16: device ID,
   476                                  leer_registro_pci 0                 ; Bits 15-0: vendor ID.
   476 0000111C 66B800000000        <1>  mov eax, %1
   476 00001122 6609F0              <1>  or eax, esi
   476 00001125 BAF80C              <1>  mov dx, 0x0CF8
   476 00001128 66EF                <1>  out dx, eax
   476 0000112A BAFC0C              <1>  mov dx, 0x0CFC
   476 0000112D 66ED                <1>  in eax, dx
   477 0000112F 83F8FF                      cmp     ax, 0xFFFF              ; Vendor ID 0xFFFF no existe.
   478 00001132 7509                        jnz     dispositivo_hallado
   479 00001134 6681C600080000              add     esi,0x0800              ; Ir al siguiente dispositivo
   480 0000113B EBD4                        jmp     ciclo_recorrer_bus_pci
   481                                  
   482                                  dispositivo_hallado:                ; Leer registro 0x0C: Bit 23: multiple function bit
   483                                      leer_registro_pci 0x0C
   483 0000113D 66B80C000000        <1>  mov eax, %1
   483 00001143 6609F0              <1>  or eax, esi
   483 00001146 BAF80C              <1>  mov dx, 0x0CF8
   483 00001149 66EF                <1>  out dx, eax
   483 0000114B BAFC0C              <1>  mov dx, 0x0CFC
   483 0000114E 66ED                <1>  in eax, dx
   484 00001150 B508                        mov     ch, 8                   ; Cantidad de funciones a procesar.
   485 00001152 660FBAE017                  bt      eax, 23
   486 00001157 7202                        jc      procesar_funcion
   487 00001159 B501                        mov     ch, 1
   488                                      
   489                                  procesar_funcion:                   ; Leer registro 0: Bits 31-16: device ID,
   490                                      leer_registro_pci 0             ; Bits 15-0: vendor ID.
   490 0000115B 66B800000000        <1>  mov eax, %1
   490 00001161 6609F0              <1>  or eax, esi
   490 00001164 BAF80C              <1>  mov dx, 0x0CF8
   490 00001167 66EF                <1>  out dx, eax
   490 00001169 BAFC0C              <1>  mov dx, 0x0CFC
   490 0000116C 66ED                <1>  in eax, dx
   491 0000116E 83F8FF                      cmp     ax, 0xFFFF              ; Vendor ID 0xFFFF no existe.
   492 00001171 7523                        jnz     funcion_hallada   
   493                                  
   494                                  ir_a_siguiente_funcion:
   495 00001173 6681C600010000              add     esi, 0x0100             ; Ir a la siguiente función
   496 0000117A FECD                        dec     ch
   497 0000117C 75DD                        jnz     procesar_funcion
   498 0000117E 6681EE00010000              sub     esi, 0x0100             ; Ir al siguiente dispositivo
   499 00001185 6681CE00070000              or      esi, 0x0700
   500 0000118C 6681C600010000              add     esi, 0x0100
   501 00001193 E97BFF                      jmp     ciclo_recorrer_bus_pci
   502                                  
   503                                  funcion_hallada:                    ; Leer registro 0x0C: Bits 22-16: header type
   504                                      leer_registro_pci 0x0C
   504 00001196 66B80C000000        <1>  mov eax, %1
   504 0000119C 6609F0              <1>  or eax, esi
   504 0000119F BAF80C              <1>  mov dx, 0x0CF8
   504 000011A2 66EF                <1>  out dx, eax
   504 000011A4 BAFC0C              <1>  mov dx, 0x0CFC
   504 000011A7 66ED                <1>  in eax, dx
