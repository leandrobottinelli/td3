   507 000011B1 66B808000000        <1>  mov eax, %1
   507 000011B7 6609F0              <1>  or eax, esi
   507 000011BA BAF80C              <1>  mov dx, 0x0CF8
   507 000011BD 66EF                <1>  out dx, eax
   507 000011BF BAFC0C              <1>  mov dx, 0x0CFC
   507 000011C2 66ED                <1>  in eax, dx
   508 000011C4 B110                        mov     cl, 0x10                ; Apuntar al primer Base Address Register (BAR)
   509                                  
   510                                  ciclo_llenar_BAR:
   511 000011C6 660FB6C1                    movzx   eax, cl
   512 000011CA 6609F0                      or      eax, esi 
   513 000011CD BAF80C                      mov     dx, 0x0CF8
   514 000011D0 66EF                        out     dx, eax
   515 000011D2 66B8FFFFFFFF                mov     eax, 0xFFFFFFFF         ; Escribir 0xFFFFFFFF en el BAR
   516 000011D8 BAFC0C                      mov     dx, 0x0CFC  
   517 000011DB 66EF                        out     dx, eax
   518 000011DD 660FB6C1                    movzx   eax, cl
   519                                      leer_registro_pci eax           ; Leerlo.
   519 000011E1 6689C0              <1>  mov eax, %1
   519 000011E4 6609F0              <1>  or eax, esi
   519 000011E7 BAF80C              <1>  mov dx, 0x0CF8
   519 000011EA 66EF                <1>  out dx, eax
   519 000011EC BAFC0C              <1>  mov dx, 0x0CFC
   519 000011EF 66ED                <1>  in eax, dx
   520 000011F1 A801                        test    al, 1                   ; Ver si el dispositivo está mapeado en memoria.
   521 000011F3 741E                        jz      mapeado_en_memoria
   522 000011F5 83E0FC                      and     ax, 0xFFFC              ; Eliminar los dos bits que no se usan para ports.
   523 000011F8 7445                        jz      ajustar_siguiente_BAR   ; El dispositivo no usa este BAR.
   524 000011FA 66F7D0                      not     eax                     ; Convertir en cantidad de bytes que se usa - 1.
   525 000011FD 6689C3                      mov     ebx, eax
   526 00001200 4D                          dec     bp                      ; Poner BP en dirección múltiplo de la cantidad
   527 00001201 09C5                        or      bp, ax                  ; de bytes que usa esta función.
   528 00001203 45                          inc     bp
   529 00001204 660FB6C1                    movzx   eax, cl
   530 00001208 6609F0                      or      eax, esi 
   531 0000120B BAF80C                      mov     dx, 0x0CF8
   532 0000120E 6689E8                      mov     eax, ebp
   533 00001211 EB22                        jmp     ajustar_BAR
   534                                  
   535                                  mapeado_en_memoria:
   536 00001213 6683E0F0                    and     eax, 0xFFFFFFF0         ; Eliminar los 4 bits que no se usan para puntero.
   537 00001217 7426                        jz      ajustar_siguiente_BAR   ; El dispositivo no usa este BAR.
   538 00001219 66F7D0                      not     eax                     ; Convertir en cantidad de bytes que se usa - 1.
   539 0000121C 6689C3                      mov     ebx, eax
   540 0000121F 664F                        dec     edi                     ; Poner EDI en dirección múltiplo de la cantidad
   541 00001221 6609C7                      or      edi, eax                ; de bytes que usa esta función.
   542 00001224 6647                        inc     edi
   543 00001226 660FB6C1                    movzx   eax, cl
   544 0000122A 6609F0                      or      eax, esi 
   545 0000122D BAF80C                      mov     dx, 0x0CF8
   546 00001230 66EF                        out     dx, eax
   547 00001232 6689F8                      mov     eax, edi
   548                                  
   549                                  ajustar_BAR:
   550 00001235 BAFC0C                      mov     dx, 0x0CFC  
   551 00001238 66EF                        out     dx, eax
   552 0000123A 6601DD                      add     ebp, ebx                ; Apuntar a puerto después de esta función.
   553 0000123D 6645                        inc     ebp
   554                                  
   555                                  ajustar_siguiente_BAR:
   556 0000123F 80C104                      add     cl, 4                   ; Apuntar al siguiente BAR.
   557 00001242 80F928                      cmp     cl, 0x28                ; Todos los BAR listos?
   558 00001245 0F857DFF                    jnz     ciclo_llenar_BAR
   559 00001249 E927FF                      jmp     ir_a_siguiente_funcion
