    15                                  
    16                                  %macro      leer_registro_pci 1                 ; 1 parámetro
    17                                      mov     eax, %1
    18                                      or      eax, esi
    19                                      mov     dx, 0x0CF8
    20                                      out     dx, eax                             ; Escribir palabra de configuración.
    21                                      mov     dx, 0x0CFC
    22                                      in      eax, dx                             ; Verificar si existe el dispositivo.
    23                                  %endmacro
